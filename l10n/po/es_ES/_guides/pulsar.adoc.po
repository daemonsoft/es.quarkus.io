#, no-wrap
msgid ""
"import io.smallrye.mutiny.Multi;\n"
"import org.eclipse.microprofile.reactive.messaging.Outgoing;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/pulsar.adoc:492
#, no-wrap
msgid ""
"import jakarta.enterprise.context.ApplicationScoped;\n"
