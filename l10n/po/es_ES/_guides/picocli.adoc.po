#: upstream/_guides/picocli.adoc:159
#, no-wrap
msgid ""
"@TopCommand\n"
"@CommandLine.Command\n"
"public class EntryCommand implements Runnable {\n"
"    @CommandLine.Spec\n"
"    CommandLine.Model.CommandSpec spec;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/picocli.adoc:165
#, no-wrap
msgid ""
"    @Override\n"
